<?xml version="1.0" encoding="utf-8"?><!-- RSRadioButton SM - State Selector -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Disabled + Checked -->
    <item android:drawable="@drawable/rs_radio_checked_disabled_sm" android:state_checked="true" android:state_enabled="false" />

    <!-- Disabled + Unchecked -->
    <item android:drawable="@drawable/rs_radio_unchecked_disabled_sm" android:state_checked="false" android:state_enabled="false" />

    <!-- Enabled + Checked -->
    <item android:drawable="@drawable/rs_radio_checked_sm" android:state_checked="true" />

    <!-- Enabled + Unchecked (Default) -->
    <item android:drawable="@drawable/rs_radio_unchecked_sm" />

</selector>
